<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:background="@color/azul"
        tools:context="car.parking.system.room.SobreEquipaActivity">


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true">

        <TextView
            android:id="@+id/developers"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:text="@string/about_text_Equipa1"
            android:textColor="@color/purple_700"
            android:textSize="@dimen/activity_size1"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="242dp"
            android:layout_height="195dp"
            android:layout_below="@+id/developers"
            android:layout_alignParentTop="true"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="56dp"
            app:srcCompat="@drawable/estg" />

        <TextView
            android:id="@+id/about_text_Proj2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/imageView2"
            android:text="@string/about_text_Proj2"
            android:textColor="@color/purple_700"
            android:textSize="@dimen/activity_horizontal_margin"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/about_text_Equipa1_fern1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/about_text_Proj2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="7dp"
            android:autoSizeMaxTextSize="@dimen/text_margin"
            android:text="@string/about_text_Equipa1_fern1"
            android:textColor="@color/purple_700"
            android:textStyle="bold"
            tools:ignore="UnknownIdInLayout" />


        <TextView
            android:id="@+id/about_text_Equipa1_Jose1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/about_text_Equipa1_fern1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="7dp"
            android:autoSizeMaxTextSize="@dimen/text_margin"
            android:text="@string/about_text_Equipa1_Jose1"
            android:textColor="@color/purple_700"
            android:textStyle="bold"
            tools:ignore="UnknownIdInLayout" />

        <TextView
            android:id="@+id/about_text_Equipa1_Andr1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/about_text_Equipa1_Jose1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="7dp"
            android:autoSizeMaxTextSize="@dimen/text_margin"
            android:text="@string/about_text_Equipa1_Andr1"
            android:textColor="@color/purple_700"
            android:textStyle="bold"
            tools:ignore="UnknownIdInLayout" />

        <TextView
            android:id="@+id/about_text_Equipa1_Ricar1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/about_text_Equipa1_Andr1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="7dp"
            android:autoSizeMaxTextSize="@dimen/text_margin"
            android:text="@string/about_text_Equipa1_Ricar1"
            android:textColor="@color/purple_700"
            android:textStyle="bold"
            tools:ignore="UnknownIdInLayout" />

        <TextView
            android:id="@+id/about_text_Equipa1_Hugo1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/about_text_Equipa1_Ricar1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="7dp"
            android:autoSizeMaxTextSize="@dimen/text_margin"
            android:text="@string/about_text_Equipa1_Hugo1"
            android:textColor="@color/purple_700"
            android:textStyle="bold"
            tools:ignore="UnknownIdInLayout" />

        <TextView
            android:id="@+id/about_text_Equipa1_fern2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/about_text_Equipa1_Hugo1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="7dp"
            android:autoSizeMaxTextSize="@dimen/text_margin"
            android:text="@string/about_text_Equipa1_1"
            android:textColor="@color/purple_700"
            android:textStyle="bold"
            tools:ignore="UnknownIdInLayout" />


        <TextView
            android:id="@+id/about_text_Equipa1_fern3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/about_text_Equipa1_fern2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="7dp"
            android:autoSizeMaxTextSize="@dimen/text_margin"
            android:text="@string/about_text_Equipa1_2"
            android:textColor="@color/purple_700"
            android:textStyle="bold"
            tools:ignore="UnknownIdInLayout" />

        <TextView
            android:id="@+id/about_text_Equipa1_fern4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/about_text_Equipa1_fern3"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="7dp"
            android:autoSizeMaxTextSize="@dimen/text_margin"
            android:text="@string/about_text_Equipa1_3"
            android:textColor="@color/purple_700"
            android:textStyle="bold"
            tools:ignore="UnknownIdInLayout" />

        <Button
            android:id="@+id/btn_close"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/about_text_Equipa1_fern4"
            android:onClick="buttonMain4"
            android:layout_marginTop="15dp"
            android:layout_centerInParent="true"
            android:text="@string/botaoClose"
            android:textAllCaps="false">
        </Button>



    </RelativeLayout>




</RelativeLayout>
